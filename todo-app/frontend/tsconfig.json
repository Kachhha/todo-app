{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "isolatedModules": true,
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": ["src"]
}
